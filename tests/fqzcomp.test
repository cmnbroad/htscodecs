#!/bin/sh
out=test.out
if test ! -d $out
then
    mkdir $out
fi

for f in $srcdir/dat/*
do
    awk '{print $1}' < $f > $out/fqz
    for s in 0 1 2 3
    do
        echo -n Testing fqzcomp_qual -s $s on $f "	"
        ./fqzcomp_qual -s $s < $out/fqz > $out/fqz.comp 2>>$out/fqz.stderr
	wc -c < $out/fqz.comp
        ./fqzcomp_qual -d < $out/fqz.comp > $out/fqz.uncomp  2>>$out/fqz.stderr
        cmp $out/fqz $out/fqz.uncomp || exit 1
    done
    echo
done
